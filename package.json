{
  "name": "league_matchups",
  "productName": "league_matchups",
  "version": "1.1.5",
  "description": "Mini template for electron-forge combine with vite and vue3.",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build:main": "vite build",
    "build:renderer": "vite build --config vite.renderer.config.mjs",
    "build": "npm run build:main && npm run build:renderer && electron-builder",
    "start": "electron-forge start",
    "start_prod": "cross-env NODE_ENV=production electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "pack": "electron-builder --dir",
    "release": "electron-builder build --publish=always",
    "lint": "echo \"No linting configured\"",
    "server": "node server.js",
    "build2": "babel src --out-dir dist",
    "test:unit": "jest"
  },
  "keywords": [],
  "author": {
    "name": "Azateq",
    "email": "not@mail.com"
  },
  "repository": "https://github.com/AgentService/LeagueMatchups.git",
  "build": {
    "appId": "com.agentservice.leaguematchups",
    "productName": "League Matchups",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "dist/main.js",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-electronegativity": "^7.2.0",
    "@electron-forge/plugin-vite": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "@types/debug": "^4.1.11",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/test-utils": "^2.4.2",
    "cross-env": "^7.0.3",
    "electron": "^24.8.8",
    "electron-builder": "^24.13.3",
    "jest": "^26.6.3",
    "sass": "^1.70.0",
    "vite": "^5.0.12",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "dependencies": {
    "@fightmegg/riot-api": "^0.0.18",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.4.1",
    "electron-log": "^5.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.1.8",
    "express": "^4.18.2",
    "find-process": "^1.4.7",
    "gsap": "^3.12.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "psql": "^0.0.1",
    "volar-service-vetur": "^0.0.17",
    "vue": "^3.4.15",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.5",
    "vue3-lazy": "^1.0.0-alpha.1",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  }
}
